{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "allowJs": true,
    "jsx": "react",
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ] 
}
